{
  "expo": {
    "name": "Minerman",
    "slug": "minerman",
    "experiments": {
      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./assets/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
    "assetBundlePatterns": ["**/*"],
    "scheme": "bitaxe",
    "android": {
      "splash": {
        "image": "./assets/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "#C8C8C8"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "#C8C8C8"
      },
      "packageName": "io.minerman",
      "package": "io.minerman"
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/quicksand/Quicksand_400Regular.ttf",
            "node_modules/@expo-google-fonts/quicksand/Quicksand_600SemiBold.ttf"
          ]
        }
      ],
      "expo-router"
    ],
    "ios": {
      "bundleIdentifier": "io.minerman"
    }
  }
}
